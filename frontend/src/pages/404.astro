---
export const prerender = false;

const { pathname } = Astro.url;

if (!pathname.startsWith("/admin")) {
  return Astro.redirect("/");
}
---

<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <title>Page Not Found</title>
    <meta http-equiv="refresh" content="0;url=/" />
  </head>
  <body>
    <p>Redirecting to home page...</p>
  </body>
</html>
